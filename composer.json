{
  "name": "groton-school/slim-lti-gae-shim",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "sbattis@groton.org"
    }
  ],
  "license": "GPL-3.0",
  "require": {
    "groton-school/slim-lti-shim": "dev-main",
    "packbackbooks/lti-1p3-tool": "^6.2",
    "google/cloud-firestore": "^1.48",
    "slim/slim": "^4.14",
    "slim/psr7": "^1.7",
    "php-di/php-di": "^7.0",
    "google/cloud-logging": "^1.32",
    "google/appengine-php-sdk": "^2.2",
    "vlucas/phpdotenv": "^5.6",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "GrotonSchool\\Slim\\LTI\\GAE\\": "src/"
    }
  },
  "scripts": {
    "release": "conventional-changelog --commit"
  },
  "require-dev": {
    "franzl/studio": "^0.16.0",
    "marcocesarato/php-conventional-changelog": "^1.17"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "franzl/studio": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
